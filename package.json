    "prettier": "^1.13.7"
  },
  "engines": {
    "node": "^6.14.0 || ^8.10.0 || >=9.5.0",
    "yarn": ">=1.7.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
